  "devDependencies": {
    "@neutralinojs/neu": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
